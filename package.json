{
  "name": "plumejs-webpack-testing",
  "version": "1.0.0",
  "description": "a sample testing project for plumejs and webpack",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve",
    "serve": "rimraf dist && webpack serve --config webpack/server.config.js",
    "build": "rimraf dist && webpack --config webpack/build.config.js",
    "test": "vitest run --no-threads",
    "test:coverage": "yarn test --coverage",
    "lint": "eslint . --ext .ts --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@plumejs/core": "^4.0.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@vitest/coverage-v8": "^0.32.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.3",
    "jsdom": "^22.1.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.5",
    "url-loader": "^4.1.1",
    "vitest": "^0.32.2",
    "webpack": "^5.88.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  }
}
